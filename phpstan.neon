includes:
    - vendor/phpstan/phpstan/conf/config.levelmax.neon
    - vendor/phpstan/phpstan-phpunit/extension.neon

parameters:
    tmpDir: tmp/custom-cache/
    paths:
        - src/
        - tests/
    ignoreErrors:
        - '#Argument of an invalid type array|null supplied for foreach, only iterables are supported#'
        - '#Parameter \#1 \$addresses of method Ddeboer\\Imap\\Message\\AbstractMessage::decodeEmailAddresses\(\) expects array, array|string given#'
